{
  "autoPlay": true,
  "fullScreen": false,
  "background": {
    "color": {
      "value": "#dfdfdf"
    }
  },
  "detectRetina": true,
  "fpsLimit": 60,
  "interactivity": {
    "detectsOn": "window",
    "events": {
      "onClick": {
        "enable": true,
        "mode": "push"
      },
      "onHover": {
        "enable": true,
        "mode": "grab"
      },
      "resize": true
    },
    "modes": {
      "grab": {
        "distance": 250,
        "links": {
          "color": "#397ef5",
          "opacity": 0.75
        }
      }
    }
  },
  "particles": {
    "number": {
      "value": 100,
      "density": {
        "enable": true,
        "area": 800
      }
    },
    "color": {
      "value": "#397ef5"
    },
    "shape": {
      "type": "circle"
    },
    "opacity": {
      "value": {
        "min": 0.3,
        "max": 0.6
      },
      "animation": {
        "enable": true,
        "speed": 3,
        "sync": false,
        "startValue": "random",
        "destroy": "none"
      }
    },
    "size": {
      "value": {
        "min": 0.1,
        "max": 3
      },
      "animation": {
        "enable": true,
        "speed": 20,
        "sync": false,
        "startValue": "random",
        "destroy": "none"
      }
    },
    "move": {
      "enable": true,
      "speed": 1,
      "outModes": {
        "default": "out"
      }
    },
    "links": {
      "enable": false
    },
    "life": {
      "count": 0,
      "duration": {
        "value": 0
      }
    },
    "rotate": {
      "value": 0
    },
    "repulse": {
      "enabled": false
    }
  },
  "pauseOnBlur": true
}
